<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.db.model.BasicAddressesModel" table="basic_addresses">
    <id name="addrCode" type="java.lang.String">
      <column name="addr_code"/>
    </id>
    <property name="addrName" type="java.lang.String">
      <column name="addr_name"/>
    </property>
  </class>
</hibernate-mapping>